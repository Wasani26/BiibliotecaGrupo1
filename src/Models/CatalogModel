<?php

//incompleto//
class CatalogoLibros extends connectionDB {
    private static $Id_Catalogo_Libros;
    private static $Nombre;

    //Construtor//
    public function __construct($Id_Catalogo_Libros, $Nombre) {
        $this->Id_Catalogo_Libros=$Id_Catalogo_Libros;
        $this->Nombre=$Nombre;
    }

    public function getId_Catalogo_Libros(){
        $this->Id_Catalogo_Libros;
    }

    public function getNombre(){
        $this->Nombre;
    }

    public function setId_Catalogo_Libros($Id_Catalogo_Libros): void {
        $this->Id_Catalogo_Libros=$Id_Catalogo_Libros;
    }

    public function setNombre($Nombre): void {
        $this->Nombre=$Nombre;
    }

}

?>